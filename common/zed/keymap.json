[
  {
    "bindings": {
      "left": "workspace::ActivatePaneLeft",
      "right": "workspace::ActivatePaneRight",
      "up": "workspace::ActivatePaneUp",
      "down": "workspace::ActivatePaneDown"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "alt-[": "editor::GoToPrevDiagnostic",
      "alt-]": "editor::GoToDiagnostic",
      "ctrl-alt-w": ["pane::CloseCleanItems", { "close_pinned": false }]
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "ctrl-a": "editor::MoveToEndOfLine"
    }
  },
  {
    "context": "Editor && !edit_prediction",
    "bindings": {
      "alt-\\": "editor::ShowEditPrediction"
    }
  },
  {
    "context": "VimControl && !menu",
    "bindings": {
      "space l": "workspace::ToggleLeftDock",
      "space r": "workspace::ToggleRightDock",
      "space p": "project_panel::ToggleFocus",
      "space o": "outline_panel::ToggleFocus",
      "space c a": "editor::ToggleCodeActions",
      "space g": "git::Branch",
      "space `": "terminal_panel::ToggleFocus",
      "space g d": "editor::GoToDefinition",
      "space g D": "editor::GoToDeclaration",
      "h": "vim::Left",
      "t": "vim::Right",
      "g": "vim::Up",
      "m": "vim::Down",
      "ctrl-g": ["editor::MoveUpByLines", { "lines": 50 }],
      "ctrl-m": ["editor::MoveDownByLines", { "lines": 50 }],
      "alt-h": "vim::PreviousWordStart",
      "alt-t": "vim::NextWordStart",
      "alt-g": "editor::MoveToStartOfParagraph",
      "alt-m": "editor::MoveToEndOfParagraph",
      "ctrl-d": ["editor::SelectNext", { "replace_newest": true }],
      "ctrl-w": "pane::CloseActiveItem"
    }
  }
]
